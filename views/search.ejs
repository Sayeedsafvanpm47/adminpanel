

<%- include('head') -%>

<h1>Search Results</h1>

<div class="table-responsive">

<% if (searchres != '') { %>
 
<table class="table table-striped">

          <thead>
                    <tr>
                              <th scope="col">Email</th>
                              <th scope="col">Password</th>

                    </tr>
          </thead>

          <tbody>
          <% searchres.forEach(element => {  %>
                    <tr>
                    <td><%= element.email %></td>
                    <td><%= element.password %></td>
          </tr>
                    <% }) %>
          </tbody>

</table>



<% } else { %>
 <h1>No results found</h1>
<% } %>

</div>


<%- include('foot') -%>